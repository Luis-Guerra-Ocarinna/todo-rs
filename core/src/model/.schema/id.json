{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Definition of ID",
    "definitions": {
        "id": {
            "type": "string",
            "pattern": "^[0-9a-z]{4}$"
        },
        "uuid": {
            "type": "string",
            "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
        }
    }
}